<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="awele.javafx.controller.ScoreTablePane">
    <userData>
  <fx:reference source="controller"/>
</userData>
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" styleClass="window-title" text="SCORES" textAlignment="CENTER" VBox.vgrow="NEVER" />
        <TableView fx:id="scoreTable" styleClass="score-table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="300.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="dateColumn" styleClass="text-column" text="Date" />
                <TableColumn fx:id="durationColumn" styleClass="number-column" text="Duration" />
                <TableColumn fx:id="winnerColumn" styleClass="text-column" text="Winner" />
                <TableColumn fx:id="humanScoreColumn" styleClass="number-column" text="Human score" />
                <TableColumn fx:id="machineScoreColumn" styleClass="number-column" text="Machine score" />
            </columns>
        </TableView>
        <Button fx:id="goBackButton" styleClass="large-button" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#goBack" text="GO_BACK" VBox.vgrow="NEVER" />
    </children>
</VBox>
